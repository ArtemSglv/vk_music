@model IEnumerable<VK_Music.Models.PhotoList>

@{
    ViewBag.Title = "TrackList";
}

<h2>My Photo</h2>
<p>Token is <b>@ViewBag.Token</b></p>

@using (@Html.BeginForm("Download", "Home"))
{
<table>
    <tr>
        <th>Photo</th>
        <th>Likes</th>
        <th>Download?</th>
    </tr>
    @foreach (var p in Model)
    {
        <tr>
            <td> <img src="@p.photo.Path" /></td>
            <td> @p.photo.Likes</td>
            <td> @Html.CheckBoxFor(m => p.isChecked)</td>
        </tr>
    }
</table>
<br/>
<input type="submit" name="Download" value="Download selected" />
}